{
    "name": "linksup",
    "displayName": "linksup",
    "description": "linksup",
    "version": "0.0.1",
    "publisher": "linksup",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [{
                "command": "extension.linksup.lnk2sup",
                "title": "链接转角标",
                "category": "Transform"
            },
            {
                "command": "extension.linksup.sup2lnk",
                "title": "角标转链接",
                "category": "Transform"
            }
        ]
    },
    "main": "./out/extension",
    "activationEvents": [
        "onLanguage:markdown",
        "onCommand:extension.linksup"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1"
    },
    "dependencies": {
        "vscode": "^1.1.21"
    }
}