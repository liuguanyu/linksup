{
    "name": "qiwoo-article-linker",
    "displayName": "WordCount",
    "description": "WordCount",
    "version": "0.0.1",
    "publisher": "WordCount",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.lnk2sup",
                "title": "链接转角标",
                "category": "Transform"
            },
            {
                "command": "extension.sup2lnk",
                "title": "角标转链接",
                "category": "Transform"
            }
        ]
    },
    "main": "./out/extension",
    "activationEvents": [
        "onLanguage:markdown"
    ],
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {}
}
